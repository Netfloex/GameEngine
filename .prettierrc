{
	"trailingComma": "all",
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"tabWidth": 4,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrder": [
		".*scss$",
		"^\\w+",
		"^@components.*",
		"^@hooks$",
		"^(@lib)|(@utils)|(@server)$",
		"^@formats$",
		"^@seo-default$"
	],
	"importOrderSeparation": true
}
